\end{frame}


\section{One Section}
\begin{frame}
  \frametitle{frame title}
  \framesubtitle{frame subtitle}
  \begin{columns}
    \begin{column}{0.46 \textwidth}
      \begin{itemize}
        \item fact 1
        \item fact 2
      \end{itemize}
    \end{column}
    \begin{column}{0.46 \textwidth}
      \begin{enumerate}
        \item numbered fact
        \item another numbered fact
      \end{enumerate}
    \end{column}    
  \end{columns}
\end{frame}

\section{Another Section}
